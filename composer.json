{
    "name": "bluemesa/acl-bundle",
    "type": "symfony-bundle",
    "description": "Acl bundle for BlueMesa projects",
    "keywords": ["bluemesa", "ACL"],
    "license": "MIT",
    "authors": [
        {
            "name": "Radoslaw Kamil Ejsmont",
            "email": "radoslaw@ejsmont.net"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/BlueMesa/AclBundle/graphs/contributors"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bluemesa\\Bundle\\AclBundle\\": ""
        }
    },
    "target-dir": "Bluemesa/Bundle/AclBundle",
    "require": {
        "php": ">=5.6.0",
        "symfony/framework-bundle": "~2.7",
        "jms/di-extra-bundle": "~1.6.0",
        "jms/security-extra-bundle": "~1.5.1",
        "bluemesa/core-bundle": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "5.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "../core-bundle/"
        }
    ]
}
