{
  "name": "bluemesa/acl-bundle",
  "type": "symfony-bundle",
  "description": "Acl bundle for BlueMesa projects",
  "keywords": ["bluemesa", "ACL"],
  "license": "MIT",
  "authors": [
    {
      "name": "Radoslaw Kamil Ejsmont",
      "email": "radoslaw@ejsmont.net"
    },
    {
      "name": "Community contributions",
      "homepage": "https://github.com/BlueMesa/AclBundle/graphs/contributors"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bluemesa\\Bundle\\AclBundle\\": ""
    }
  },
  "require": {
    "php": ">=5.6.0",
    "symfony/framework-bundle": "~3.0",
    "symfony/security-acl": "~3.0",
    "symfony/form": "~3.0",
    "symfony/validator": "~3.0",
    "sensio/framework-extra-bundle": "~3.0",
    "doctrine/common": "~2.5",
    "doctrine/orm": "~2.5",
    "jms/di-extra-bundle": "~1.6",
    "jms/security-extra-bundle": "~1.5",
    "bluemesa/core-bundle": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "5.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "../core-bundle/"
    }
  ]
}
